{
    "name": "Timesheet.bITe",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "Will post data to the service",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/background.js",
            "scripts/lib/jquery-2.0.3.min.js"
        ],
        "persistent": false
    },
    "permissions": [
        "http://timesheetservice.herokuapp.com/",
        "http://localhost:3000/",
        "identity",
        "alarms",
        "geolocation"
    ],
    "oauth2": {
        "client_id": "149255324323-hgjmq59gnt94jpp7sqc083qetqc55qbd.apps.googleusercontent.com",
        "scopes": ["https://www.googleapis.com/auth/userinfo.profile"]
    }
}